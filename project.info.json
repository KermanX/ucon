{
  "classes": {
    "jest": {
      "commands": {
        "test": {
          "runAt": "<CurrentProject>",
          "commands": [
            "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --roots <CurrentPackage>/dist-test/",
            "echo Hello world!"
          ]
        }
      }
    },
    "ts": {
      "commands": {
        "compile": "tsc --build",
        "build": "tsc --build",
        "compile:test": "tsc --build __tests__"
      },
      "runners": {
        "test": ["compile:test", "test"]
      }
    },
    "react": {
      "commands": {
        "compile": null,
        "start": "npx react-scripts start",
        "build": "npx react-scripts build"
      }
    }
  },
  "packages": {
    "core": {
      "path": "./core",
      "class": "ts"
    },
    "roadmap": {
      "path": "./project/roadmap",
      "class": "react"
    },
    "scripts": {
      "path": "./scripts",
      "class": "ts",
      "commands": {
        "start": "node ./dist/index.js -h"
      }
    },
    "focus-system": {
      "path": "./utils/focus-system",
      "class": "react"
    },
    "range-set": {
      "path": "./utils/range-set",
      "class": "ts",
      "commands": {
        "compile": null,
        "start": "ts-node ./src/index.ts"
      }
    },
    "utty": {
      "path": "./utty/utty",
      "class": "ts"
    },
    "utty-node": {
      "path": "./utty/utty-node/utty-node",
      "class": "ts jest"
    },
    "nodeliketty": {
      "path": "./utty/utty-node/nodeliketty",
      "class": "ts"
    },
    "nodeliketty-testimpl": {
      "path": "./utty/utty-node/nodeliketty-testimpl",
      "class": "ts"
    }
  }
}
