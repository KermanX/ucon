{
  "classes": {
    "ts": {
      "commands": {
        "compile": "tsc",
        "build": "tsc"
      }
    },
    "react": {
      "commands": {
        "compile": null,
        "start": "npx react-scripts start",
        "build": "npx react-scripts build"
      }
    }
  },
  "packages": {
    "core": {
      "path": "./core",
      "class": "ts"
    },
    "roadmap": {
      "path": "./project/roadmap",
      "class": "react"
    },
    "scripts": {
      "path": "./scripts",
      "class": "ts",
      "commands": {
        "start": "node ./dist/index.js -h"
      }
    },
    "focus-system": {
      "path": "./utils/focus-system",
      "class": "react"
    },
    "range-set": {
      "path": "./utils/range-set",
      "class": "ts",
      "commands": {
        "compile": null,
        "start": "ts-node ./src/index.ts"
      }
    },
    "utty": {
      "path": "./utty/utty",
      "class": "ts"
    }
  }
}
